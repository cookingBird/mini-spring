<?xml version="1.0" encoding="UTF-8" ?>
<beans>
    <bean id="aservice" class="test.AServiceImpl">
        <constructor-arg type="String" name="name" value="abc"/>
        <constructor-arg type="int" name="level" value="3"/>
        <property type="String" name="property1" value="Someone says"/>
        <property type="String" name="property2" value="Hello World!"/>
        <property type="test.BaseService" name="ref1" ref="baseservice"/>
    </bean>
    <bean id="baseservice" class="test.BaseService">
        <!--        <property type="test.BaseBaseService" name="baseBaseService" ref="basebaseservice"/>-->
    </bean>
    <bean id="basebaseservice" class="test.BaseBaseService">
        <property type="test.AServiceImpl" name="as" ref="aservice"/>
    </bean>
    <bean id="contextListener" class="test.MyListener"/>

    <bean id="beanFactoryPostProcessor" class="test.MyBeanFactoryPostProcessor"/>

    <bean id="autowiredAnnotationBeanPostProcessor"
          class="beans.factory.annotation.AutowiredAnnotationBeanPostProcessor"/>
</beans>